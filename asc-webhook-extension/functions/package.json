{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "test": "jest --silent=false",
    "build": "tsc",
    "serve": "npm run build && npm run env && firebase functions:config:get > .runtimeconfig.json && firebase emulators:start --only functions:ascWebhookExtension",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "env": "firebase functions:config:unset env && firebase functions:config:set env=\"$(cat env.json)\"",
    "deploy": "npm run build && npm run env &&firebase deploy --only functions:ascWebhookExtension",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^0.21.1",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.3",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "typescript": "^3.8.0"
  },
  "private": true
}
